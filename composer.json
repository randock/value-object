{
  "name": "randock/value-object",
  "description": "Randock Value Objects",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Randock\\ValueObject\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Randock\\ValueObject\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "7.1"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "giggsey/libphonenumber-for-php": "^8.7",
    "ramsey/uuid": "^3.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0"
  }
}
